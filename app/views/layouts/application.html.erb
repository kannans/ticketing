<!DOCTYPE html>
<html>
  <head>
    <title>Octopaas Ticketing App</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
    <%= yield %>

    <% if content_for?(:page_js) %>
      <%= yield(:page_js) %>
    <% end %>
  </body>
</html>
