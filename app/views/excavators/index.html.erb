<p id="notice"><%= notice %></p>

<h1>Excavators</h1>

<table>
  <thead>
    <tr>
      <th>Company name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @excavators.each do |excavator| %>
      <tr>
        <td><%= excavator.company_name %></td>
        <td><%= link_to 'Show', excavator %></td>
        <td><%= link_to 'Edit', edit_excavator_path(excavator) %></td>
        <td><%= link_to 'Destroy', excavator, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Excavator', new_excavator_path %>
